p=int(input("Enter the Principle Amount:"))
n=int(input("Enter the Number of Years:"))
SC=input("Are you a Senior Citizen Y/N:")
G=input("Enter your Gender M/F:")
if SC=='Y' and G=='M':
    print("SI=",(p*n*12)/100)
elif SC=='Y' and G=='F':
    print("SI=",(p*n*15)/100)
else:
    print("SI=",(p*n*10)/100)
